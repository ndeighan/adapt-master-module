{{! Maintainers - Kevin Corry, Daryl Hedley}}
<div class="tutor">
    
    <div class="tutor-toolbar">
        <a href="#" class="tutor-done">
            <div class="tutor-icon-close icon icon-cross"></div>
        </a>
    </div>
    <div class="tutor-content">
            <div class="tutor-audio">
                {{#if feedbackAudioMp3}}
            <audio src="{{{feedbackAudioMp3}}}" type="audio/mp3" autoplay />
                {{else}}
            <audio src="{{{feedbackAudioOgg}}}" type="audio/ogg" autoplay />
                {{/if}}
            </div>
			
		{{#if feedbackGraphic}}
        <div class="tutor-image">
                <img src="{{{feedbackGraphic}}}" alt="{{{feedbackGraphicAlt}}}" title="{{{feedbackGraphicTitle}}}"/>
        </div>
		{{/if}}
        <div class="tutor-inner">
                {{#if feedbackTitle}}
                <div class="tutor-title">
                    <h5 class="tutor-title-inner">
                    {{{feedbackTitle}}}
                    </h5>
                </div>
                {{/if}}
            <div class="tutor-body">
                <div class="tutor-body-inner">{{{feedbackMessage}}}</div>
            </div>
        </div>
    </div>
</div>

<div class="tutor-shadow"></div>
